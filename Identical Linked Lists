def areIdentical(head1, head2):
    # Code here
    current1 = head1
    current2 = head2
    while current1 !=None and current2!=None:
        if(current1.data != current2.data):
            return False
        current1=current1.next
        current2 = current2.next
    return (current1 ==None and current2==None)
